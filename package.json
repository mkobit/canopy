{
  "name": "canopy-graph",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "packages/*",
    "apps/*",
    "tools"
  ],
  "engines": {
    "bun": "1.2.14"
  },
  "scripts": {
    "build": "tsc -b && bun --filter './apps/*' build",
    "check:versions": "bun tools/verify-versions.ts",
    "test": "vitest",
    "lint": "bun run check:versions && eslint .",
    "format": "prettier --write .",
    "typecheck": "bun --filter '*' typecheck",
    "prepare": "husky",
    "bd": "bd",
    "postinstall": "bd hooks install || true"
  },
  "devDependencies": {
    "@beads/bd": "^0.47.1",
    "@stylistic/eslint-plugin": "^5.7.0",
    "@types/node": "^25.0.7",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "@types/uuid": "^11.0.0",
    "@typescript-eslint/eslint-plugin": "8.54.0",
    "@typescript-eslint/parser": "7.18.0",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitest/coverage-v8": "1.6.0",
    "electron": "^40.0.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-functional": "^9.0.2",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-unicorn": "^54.0.0",
    "globals": "^17.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.0",
    "tsx": "^4.21.0",
    "typescript": "5.9.3",
    "typescript-eslint": "8.53.0",
    "uuid": "^13.0.0",
    "vite": "^7.3.1",
    "vitest": "1.6.0"
  },
  "dependencies": {
    "react": "^19.2.3",
    "react-dom": "^19.2.3"
  }
}
